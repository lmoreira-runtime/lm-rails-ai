<h1 style="margin-top: 20px;">
  Import Questions</h1>

<%= form_with model: @question, local: true do |form| %>
  <div class="form-group">
    <%= form.label :questions, "Enter your questions" %><br>
    <%= form.text_area :questions, rows: 10, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.submit "Import Questions", class: "btn btn-primary" %>
  </div>
<% end %>
